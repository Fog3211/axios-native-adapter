{
  "name": "request",
  "version": "0.1.12",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/request.esm.js",
  "scripts": {
    "build": "tsdx build",
    "build:docs": "dumi build",
    "clean": "rm -rf ./dist ./docs-dist",
    "start": "dumi dev",
    "test": "tsdx test --coverage --watch",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "keywords": [
    "axios",
    "request"
  ],
  "author": "Fog3211",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/request.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/request.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "axios": "^1.3.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/axios": "^0.14.0",
    "@umijs/fabric": "^3.0.0",
    "dumi": "^2.1.15",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "query-string": "^8.1.0"
  },
  "peerDependencies": {
    "axios": "^0.27.2 || ^1.0.0"
  }
}
